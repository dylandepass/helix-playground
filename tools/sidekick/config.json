{
  "project": "helix-playground",
  "sourceFormat": {
    "name": "Document Authoring",
    "pattern": "https://custom.live/edit#/{{org}}/{{repo}}{{pathname}}"
  },
  "plugins": [
    {
      "id": "franklin-library",
      "title": "Library",
      "environments": ["edit"],
      "isPalette": true,
      "paletteRect": "top: auto; bottom: 20px; left: 50px; height: 398px; width: 360px;",
      "url": "/library/library?suppressFrame=true",
      "includePaths": ["**.docx**"]
    },
    {
      "id": "publish",
      "confirm": true
    }
  ]
}